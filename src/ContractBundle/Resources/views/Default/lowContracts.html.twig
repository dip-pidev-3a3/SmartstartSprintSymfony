{% extends "SmartStartBase.html.twig" %}
{% block title %}My Contracts{% endblock %}
{% block body %}
    <section class="bg-title-page flex-c-m p-t-160 p-b-80 p-l-15 p-r-15" style="background-image: url(https://www.insidehighered.com/sites/default/server_files/media/contracts%20finkelstein.jpg);">
        <h2 class="tit6 t-center">
        </h2>
    </section>

    <section>
        <div class="bread-crumb bo5-b p-t-17 p-b-17">
            <div class="container">
                <a href="{{ path('my_contracts') }}" class="txt27">
                    My Contracts
                </a>
                <span class="txt29 m-l-10 m-r-10">/</span>
                <a href="{{ path('my_low_contracts') }}" class="txt27">
                    Auto-Generated Contracts
                </a>
            </div>
        </div>
    </section>


    <div class="container">
        {% for i in con %}
            <div class="col-lg-4 col-md-6 mb-4" style="display: inline-block">
                <div class="card h-100">
                    <img class="card-img-top" src="data:image/jpeg;base64," alt="">
                    <div class="card-body">
                        <h4 class="card-title">
                            <a href="#">{{ i.idApplication.idFreelancer.name }} {{ i.idApplication.idFreelancer.lastName }}</a>
                        </h4>
                        <h5>{{ i.idApplication.idOpportunity.jobTitle }}</h5>
                        <p class="card-text">{{ i.idApplication.idOpportunity.jobDescription }}</p>
                    </div>
                    <div class="card-footer">
                        <strong class="text-muted">
                            <a href="#updateContract" class="btn3 flex-c-m size13 txt11 trans-0-4 m-l-r-auto"
                               data-toggle="modal"><i class="material-icons">&#xE147;</i> Complete...</a>
                        </strong>
                    </div>
                </div>
            </div>
            <!-- Add Modal HTML -->
            <div id="updateContract" class="modal fade">
                <div class="modal-dialog">
                    <div class="modal-content">
                        <form name="addForm" id="addForm" method="post" action="{{ path('update_contract',{'id':i.idContract}) }}">
                            <div class="modal-header">
                                <h4 class="modal-title">Complete Your Contract...
                                    <br/>
                                    <small style="color: red">pls notice that all fields are required * and the Start Date must be chosen before the Finish Date</small>
                                </h4>
                                <button type="button" class="close" data-dismiss="modal" aria-hidden="true">&times;</button>
                            </div>

                            <div class="modal-body">
                                <div class="form-group">
                                    <label>Job Title</label>
                                    <input type="text" id="idApplication" name="idApplication" class="form-control" disabled="true" value="{{ i.idApplication.idOpportunity.jobTitle }}">

                                </div>
                                <div class="form-group">
                                    <label>Freelancer Name</label>
                                    <input type="text" id="freelancer" name="freelancer" class="form-control" disabled="true" value="{{ i.idApplication.idFreelancer.name }} {{ i.idApplication.idFreelancer.lastName }}">

                                </div>
                                <div class="form-group">
                                    <label>Start Date</label>
                                    <input type="date" id="start" name="start" class="form-control">
                                    <div id="errors1" class="help-block" role="alert" style="color: red"></div>

                                </div>
                                <div class="form-group">
                                    <label>Finish Date</label>
                                    <input type="date" id="finish" name="finish" class="form-control">
                                    <div id="errors2" class="help-block" role="alert" style="color: red"></div>

                                </div>
                                <div class="form-group">
                                    <label>Sum</label>
                                    <input type="number" step="0.01" id="sum" name="sum" min="100" class="form-control" placeholder="Type The Sum Here">
                                    <div id="errors3" class="help-block" role="alert" style="color: red"></div>

                                </div>
                                <div class="form-group">
                                    <label>Payment Method (Default is Online)</label>
                                    <select id="paymentMethod" name="paymentMethod" class="form-control">
                                        <option value="En Ligne">Online</option>
                                        <option value="Cash">Cash</option>
                                        <option value="Cheque">Cheque</option>
                                    </select>

                                </div>
                            </div>
                            <div class="modal-footer">
                                <input type="button" class="btn btn-default" data-dismiss="modal" value="Cancel">
                                <input type="submit" id="valider" name="valider" class="btn btn-success" id="addb" value="Add" disabled>
                            </div>
                        </form>
                    </div>
                </div>
            </div>
        {% endfor %}
    </div>
    <script src="{{ asset('assets/js/jquery-3.3.1.js') }}"></script>
    <script type="text/javascript">
        document.getElementById('sum').addEventListener('change', function() {
            var error3 = document.getElementById('errors3');
            var re = new RegExp('^(?=.+)(?:[1-9]\d*|0)?(?:\.\d+)?');

            if ((document.getElementById('sum').value.match(re)==false)||(document.getElementById('sum').value=="")) {
                error3.textContent = "The Sum field is required and can't be null or negative";
                document.getElementById('addForm').classList.add('has-error');
                document.getElementById('valider').setAttribute("disabled",null);
            } else {
                error3.textContent = '';
                document.getElementById('addForm').classList.remove('has-error');
                if((document.getElementById('finish').value!="")&&(document.getElementById('start').value!="")&&(document.getElementById('sum').value!="")){
                    document.getElementById('valider').removeAttribute("disabled");
                }
                else{
                    document.getElementById('valider').setAttribute("disabled",null);
                }
            }
        });
        document.getElementById('start').addEventListener('change', function() {
            var error1 = document.getElementById('errors1');
            var myDate = new Date(document.getElementById('start').value.toString());
            var now = new Date();
            if ((myDate<now)||(document.getElementById('start').value=="")) {
                error1.textContent = "The Start Date is required and can't be in the past or more than the Finish Date";
                document.getElementById('addForm').classList.add('has-error');
                document.getElementById('valider').setAttribute("disabled",null);
            } else {
                error1.textContent = '';
                document.getElementById('addForm').classList.remove('has-error');
                if((document.getElementById('finish').value!="")&&(document.getElementById('start').value!="")&&(document.getElementById('sum').value!="")){
                    document.getElementById('valider').removeAttribute("disabled");
                }
                else{
                    document.getElementById('valider').setAttribute("disabled",null);
                }
            }
        });
        document.getElementById('finish').addEventListener('change', function() {
            var error2 = document.getElementById('errors2');
            var startDate = new Date(document.getElementById('start').value.toString());
            var finishDate = new Date(document.getElementById('finish').value.toString());

            if ((finishDate<startDate)||(document.getElementById('finish').value=="")||(document.getElementById('start').value=="")) {
                error2.textContent = "The Finish Date is required and must be more then the Start Date and you must Select the Start Date at first";
                document.getElementById('addForm').classList.add('has-error');
                document.getElementById('valider').setAttribute("disabled",null);
            } else {
                error2.textContent = '';
                document.getElementById('addForm').classList.remove('has-error');
                if((document.getElementById('finish').value!="")&&(document.getElementById('start').value!="")&&(document.getElementById('sum').value!="")){
                    document.getElementById('valider').removeAttribute("disabled");
                }
                else{
                    document.getElementById('valider').setAttribute("disabled",null);
                }
            }
        });

    </script>
{% endblock %}